from swim_serial import SwimSerial
from swim_client import SwimClient
s = SwimSerial(baudrate = 9600)
client = SwimClient()


while True:
    client.setpayload('bbbb')
    client.send()
    
    client.receive()
    b = client.getreceived()
    
    s.setpayload(b)
    s.print
    
    print s.getreceive()